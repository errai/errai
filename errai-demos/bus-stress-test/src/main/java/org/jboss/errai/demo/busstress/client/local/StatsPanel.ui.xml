<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <g:VerticalPanel styleName="resultsPanel">
    <g:HorizontalPanel>
      <g:VerticalPanel width="157px">
        <g:Label text="---" ui:field="messageSendCount" styleName="bigcounter" width="202px" />
        <g:Label text="Messages Sent" />
        <g:Label text="--- bytes" ui:field="messageSendBytes" />
      </g:VerticalPanel>
      <g:VerticalPanel>
        <g:Label text="---" ui:field="inFlightCount" styleName="bigcounter" width="207px" />
        <g:Label text="Messages in flight" />
        <g:Label text="--- ms/resp" ui:field="inFlightAvg"/>
      </g:VerticalPanel>
      <g:VerticalPanel>
        <g:Label text="---" ui:field="messageRecvCount" styleName="bigcounter" width="232px" />
        <g:Label text="Messages Received" />
        <g:Label text="--- bytes" ui:field="messageRecvBytes" />
      </g:VerticalPanel>
    </g:HorizontalPanel>
    <g:Label text="Stats Summary" ui:field="summaryLabel" styleName="summary"/>
  </g:VerticalPanel>
</ui:UiBinder> 